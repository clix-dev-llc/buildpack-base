#!/bin/bash

export INPUT_DIR=/input
export OUTPUT_DIR=/output
export CACHE_DIR=/cache
export LOG_DIR=/log
export WORKSPACE_DIR=/workspace
export RUN_LOG=$LOG_DIR/run.log
export STDERR_LOG=$LOG_DIR/stderr.log
export ELAPSED_LOG=$LOG_DIR/elapsed.log
export OUTPUT_ARCHIVE=/output.tar.gz

mkdir $WORKSPACE_DIR
